<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="13dp"
    android:background="@drawable/searh_item_border"
    android:orientation="vertical"
    android:paddingStart="20dp"
    android:paddingBottom="30dp"
    android:weightSum="1">

    <ImageView
        android:id="@+id/map_result_benefit_btn"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_gravity="end"
        android:layout_marginTop="22dp"
        android:layout_marginEnd="22.5dp"
        android:src="@drawable/add_black" />

    <TextView
        android:id="@+id/map_result_benefit_name"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="35dp"
        android:fontFamily="@font/jalnan"
        android:lineSpacingExtra="11.5sp"
        android:paddingEnd="88.8dp"
        android:text="의료수급권자 영유아건강검진비 지원"
        android:textColor="@color/colorBlack"
        android:textSize="21.5sp" />

</LinearLayout>