<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F9F9F9"
    tools:context=".TutorialResult">

    <TextView
        android:id="@+id/BenefitText"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeTextType="none"
        android:gravity="center"
        android:textStyle="bold"
        app:autoSizePresetSizes="@array/autosize_text_sizes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.122"
        app:layout_constraintWidth_percent=".8" />

    <androidx.cardview.widget.CardView
        android:id="@+id/CardBenefit1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:cardCornerRadius="19dp"
        app:cardElevation="1.5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias=".34"
        app:layout_constraintWidth_percent=".8">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/BenefitTitle1"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:autoSizeTextType="none"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:maxLines="1"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent=".3"
                app:layout_constraintHorizontal_bias=".33"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias=".25"
                app:layout_constraintWidth_percent=".78" />

            <TextView
                android:id="@+id/BenefitTag1"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:autoSizeTextType="none"
                android:ellipsize="end"
                android:ems="10"
                android:gravity="center_vertical"
                android:maxLength="23"
                android:maxLines="1"
                app:autoSizePresetSizes="@array/autosize_text_sizes"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent=".25"
                app:layout_constraintHorizontal_bias=".28"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias=".82"
                app:layout_constraintWidth_percent=".75" />

            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/arrow_right_black"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent=".27"
                app:layout_constraintHorizontal_bias=".97"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintWidth_percent=".08" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/CardBenefit2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:gravity="center"
        app:cardCornerRadius="19dp"
        app:cardElevation="1.5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias=".51"
        app:layout_constraintWidth_percent=".8">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="330dp"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/BenefitTitle2"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:autoSizeTextType="none"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:maxLines="1"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent=".3"
                app:layout_constraintHorizontal_bias=".33"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias=".25"
                app:layout_constraintWidth_percent=".78" />

            <TextView
                android:id="@+id/BenefitTag2"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:autoSizeTextType="none"
                android:ellipsize="end"
                android:ems="10"
                android:gravity="center_vertical"
                android:maxLength="23"
                android:maxLines="1"
                app:autoSizePresetSizes="@array/autosize_text_sizes"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent=".25"
                app:layout_constraintHorizontal_bias=".28"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias=".82"
                app:layout_constraintWidth_percent=".75" />

            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/arrow_right_black"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent=".27"
                app:layout_constraintHorizontal_bias=".97"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintWidth_percent=".08" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/CardBenefit3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="12.5dp"
        android:layout_marginBottom="20dp"
        android:gravity="center"
        app:cardCornerRadius="19dp"
        app:cardElevation="1.5dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent=".13"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias=".695"
        app:layout_constraintWidth_percent=".8">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/BenefitTitle3"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:autoSizeTextType="none"
                android:ellipsize="end"
                android:gravity="center_vertical"
                android:maxLines="1"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent=".3"
                app:layout_constraintHorizontal_bias=".33"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias=".25"
                app:layout_constraintWidth_percent=".78" />

            <TextView
                android:id="@+id/BenefitTag3"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:autoSizeTextType="none"
                android:ellipsize="end"
                android:ems="10"
                android:gravity="center_vertical"
                android:maxLength="23"
                android:maxLines="1"
                app:autoSizePresetSizes="@array/autosize_text_sizes"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent=".25"
                app:layout_constraintHorizontal_bias=".28"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias=".82"
                app:layout_constraintWidth_percent=".75" />

            <ImageView
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:background="@drawable/arrow_right_black"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintHeight_percent=".27"
                app:layout_constraintHorizontal_bias=".97"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintWidth_percent=".08" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>


    <TextView
        android:id="@+id/BtnMain"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeTextType="none"
        android:clickable="true"
        android:gravity="center"
        android:text="더 많은 혜택들이 기다려요"
        android:textStyle="bold"
        app:autoSizePresetSizes="@array/autosize_text_sizes"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent=".045"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias=".81"
        app:layout_constraintVertical_chainStyle="packed"
        app:layout_constraintWidth_percent="0.55" />

    <Button
        android:id="@+id/BtnGoMain"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/radius_smallround_pink_button"
        android:text="메인으로 가기"
        android:textColor="#FFFFFF"
        android:textSize="27sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHeight_percent=".085"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias=".905"
        app:layout_constraintWidth_percent="0.8" />

</androidx.constraintlayout.widget.ConstraintLayout>