<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.psj.welfare">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="com.android.alarm.permission.SET_ALARM" />

    <application
        android:name="com.psj.welfare.kakao.App"
        android:allowBackup="true"
        android:icon="@drawable/launcher_image"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@drawable/launcher_image"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="AllowBackup,UnusedAttribute">
        <activity android:name="com.psj.welfare.Activity.TermsAndConditionsActivity"/>
        <activity android:name="com.psj.welfare.Activity.PersonalInformationActivity" />
        <activity android:name="com.psj.welfare.Activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.psj.welfare.Activity.ChoiceKeywordActivity" />
        <activity android:name="com.psj.welfare.Activity.SearchResultActivity" />
        <activity android:name="com.psj.welfare.Activity.YoutubeActivity" />
        <activity
            android:name="com.psj.welfare.Activity.ReviewUpdateActivity"
            android:theme="@style/ReviewTheme" />
        <activity android:name="com.psj.welfare.Activity.GetUserInformationActivity"/>
        <activity android:name="com.psj.welfare.Activity.CalculatorResultActivity" />
        <activity
            android:name="com.psj.welfare.Activity.MyInfoUpdateActivity"
            android:theme="@style/ToolbarTheme" />
        <activity android:name="com.psj.welfare.Activity.MapSearchActivity" />
        <activity android:name="com.psj.welfare.Activity.MapDetailActivity" />
        <activity
            android:name="com.psj.welfare.Activity.MapActivity"
            android:theme="@style/ReviewTheme" />
        <activity
            android:name="com.psj.welfare.Activity.ReviewActivity"
            android:parentActivityName="com.psj.welfare.Activity.DetailBenefitActivity"
            android:theme="@style/ReviewTheme" />
        <activity android:name="com.psj.welfare.Activity.TestActivity" />
        <activity android:name="com.psj.welfare.Activity.Compatibility.Compatibility_FourthActivity" />
        <activity android:name="com.psj.welfare.Activity.Compatibility.Compatibility_ResultActivity">
            <intent-filter>
                <action android:name="android:intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="@string/kakaolink_host"
                    android:scheme="@string/kakao_scheme" />
            </intent-filter>
        </activity>
        <activity android:name="com.psj.welfare.Activity.Compatibility.Compatibility_ThirdActivity" />
        <activity android:name="com.psj.welfare.Activity.Compatibility.Compatibility_SecondActivity" />
        <activity android:name="com.psj.welfare.Activity.Compatibility.Compatibility_FirstActivity" />

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />

        <activity android:name="com.psj.welfare.Activity.MainTabLayoutActivity" />
        <activity android:name="com.psj.welfare.Activity.ResultBenefitActivity" />
        <activity android:name="com.psj.welfare.Activity.LoginActivity" />
        <activity android:name="com.psj.welfare.Activity.DetailBenefitActivity" />

        <service
            android:name="com.psj.welfare.API.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service
            android:name="com.psj.welfare.API.MyFirebaseInstanceIDService"
            android:enabled="false"
            android:exported="false"
            tools:ignore="InnerclassSeparator" />
        <service android:name="com.google.firebase.components.ComponentDiscoveryService">
            <meta-data
                android:name="com.google.firebase.components:com.google.firebase.iid.Registrar"
                android:value="com.google.firebase.components.ComponentRegistrar" />
        </service>
    </application>

</manifest>