<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.psj.welfare">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name=".kakao.App"
        android:allowBackup="true"
        android:icon="@drawable/launcher_image"
        android:label="@string/app_name"
        android:requestLegacyExternalStorage="true"
        android:roundIcon="@drawable/launcher_image"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="AllowBackup,UnusedAttribute">
        <activity android:name=".Test.TestDetailBenefitActivity"/>
        <activity android:name=".activity.PersonalInformationActivity" />
        <activity android:name=".activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ChoiceKeywordActivity" />
        <activity android:name=".activity.SearchResultActivity" />
        <activity android:name=".activity.YoutubeActivity" />
        <activity
            android:name=".activity.ReviewUpdateActivity"
            android:theme="@style/ReviewTheme" />
        <activity android:name=".activity.GetUserInformationActivity" />
        <activity android:name=".activity.CalculatorResultActivity" />
        <activity
            android:name=".activity.MyInfoUpdateActivity"
            android:theme="@style/ToolbarTheme" />
        <activity android:name=".activity.MapSearchActivity" />
        <activity android:name=".activity.MapDetailActivity" />
        <activity
            android:name=".activity.MapActivity"
            android:theme="@style/ReviewTheme" />
        <activity
            android:name=".activity.ReviewActivity"
            android:parentActivityName=".activity.DetailBenefitActivity"
            android:theme="@style/ReviewTheme" />
        <activity android:name=".activity.TestActivity" />
        <activity android:name=".activity.Compatibility.Compatibility_FourthActivity" />
        <activity android:name=".Test.CompatibilityTestActivity" />
        <activity android:name=".activity.Compatibility.Compatibility_ResultActivity">
            <intent-filter>
                <action android:name="android:intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="@string/kakaolink_host"
                    android:scheme="@string/kakao_scheme" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.Compatibility.Compatibility_ThirdActivity" />
        <activity android:name=".activity.Compatibility.Compatibility_SecondActivity" />
        <activity android:name=".activity.Compatibility.Compatibility_FirstActivity" />

        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="@string/kakao_app_key" />

        <activity android:name=".activity.MainTabLayoutActivity" />
        <activity android:name=".activity.ResultBenefitActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.DetailBenefitActivity" />

        <service
            android:name=".api.MyFirebaseMessagingService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>
        <service
            android:name=".api.MyFirebaseInstanceIDService"
            android:enabled="false"
            android:exported="false"
            tools:ignore="InnerclassSeparator" />
        <service android:name="com.google.firebase.components.ComponentDiscoveryService">
            <meta-data
                android:name="com.google.firebase.components:com.google.firebase.iid.Registrar"
                android:value="com.google.firebase.components.ComponentRegistrar" />
        </service>
    </application>

</manifest>